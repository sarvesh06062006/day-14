#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
    int n, i, smallest;
    scanf("%d", &n);
    int a[n];
    for(i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }
    smallest = a[0];
    for(i = 1; i < n; i++) {
        if(a[i] < smallest) {
            smallest = a[i];
        }
    }
    printf("%d\n", smallest);

    return 0;
}
